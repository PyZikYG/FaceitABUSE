{
  "manifest_version": 3,
  "name": "Faceit Abuse",
  "version": "1.3",
  "description": "Faceit ABUSE",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.faceit.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto Match Finder"
  }
}